#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int n;
    scanf("%d", &n);
    while(n--){
        int a;
        scanf("%d", &a);
        if(a <= 100 && a >= 90) printf("優等\n");
        else if(a <= 89 && a >= 80) printf("甲等\n");
        else if(a <= 79 && a >= 70) printf("乙等\n");
        else if(a <= 69 && a >= 60) printf("丙等\n");
        else printf("不及格\n");
    }



	return 0;
}